					"WorkingDirectory": "$(OutDir)"
				}
			},
			"SolutionConfiguration": "Release",
			"SolutionPlatform": "x64"
		},
		"Acad Debug testing": {
			"Projects": {
				"IcArxUiTesting": {
					"CommandLineArguments": "/B \"$(ProjectDir)IcArxUiTesting.scr\"",
